package com.ldg.BubbleSort;

public class Main {

	public static void main(String[] args) {

		Integer[] array = new Integer[] { 1, 2, 5, 7, 4, 8, 1 };

		for (int end = array.length; end > 0; end--) {
			for (int j = 1; j < end; j++) {
				if (array[j] < array[j - 1]) {
					int temp = array[j];
					array[j] = array[j - 1];
					array[j - 1] = temp;
				}
			}
		}

	}

}
