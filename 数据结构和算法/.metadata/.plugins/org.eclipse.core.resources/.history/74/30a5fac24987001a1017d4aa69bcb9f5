package com.ldg.map;

public interface Map<K, V> {

	int size();

	boolean isEmpty();

	V put(K key, V value);

	V remove(V value);

	boolean containsKey(K key);

	boolean containsValue(V value);

	void traversal();

	public static abstract class Visitor<V> {

		boolean flag;

		public abstract boolean  visit(V value) {
			return false;
		}
	}

}
